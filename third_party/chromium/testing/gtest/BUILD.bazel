load(
    "//third_party/chromium:build_defs.bzl",
    "chromium_cc_library",
)

chromium_cc_library(
    name = "gtest",
    testonly = True,
    hdrs = ["include/gtest/gtest.h"],
    visibility = ["//visibility:public"],
    deps = ["@googletest//:gtest_main"],
)

chromium_cc_library(
    name = "gtest_prod",
    hdrs = ["include/gtest/gtest_prod.h"],
    visibility = ["//visibility:public"],
    deps = ["@googletest//:gtest_prod"],
)
